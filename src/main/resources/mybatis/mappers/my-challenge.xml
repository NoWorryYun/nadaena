<mapper namespace="">


	<!-- 리뷰쓰기 -->
	<insert id="insertReview" parameterType="com.javaex.vo.reviewVo">
		<![CDATA[
			insert into review
			values (seq_reviewNo.nextval,
					#{challengeNo},
					#{userNo},
					#{reviewIMG},
					#{reviewContent},
					sysdate
		]]>
	</insert>

